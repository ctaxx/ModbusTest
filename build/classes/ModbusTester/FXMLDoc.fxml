<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="anchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ModbusTester.FXMLDocController">
    <children>
        <HBox>
            <Button  text="Add items" prefWidth="150.0"  onAction="#handleButtonAction" fx:id="button" />
            <Button text="Устройство" prefWidth="150.0"  onAction="#handleAddDeviceAction"  fx:id="deviceButton"/>
            <TextField prefColumnCount="12" fx:id="ipField"/>
        </HBox>
        <!--<TableView fx:id = "table"/>-->
        <VBox prefWidth="180.0" spacing="2"  fx:id="itemsVBox"/> 
        <ProgressBar visible="false" fx:id="taskProgress"/>
    </children>
    <stylesheets>
        <URL value="@fxmldoc.css"/>
    </stylesheets>
</VBox>
